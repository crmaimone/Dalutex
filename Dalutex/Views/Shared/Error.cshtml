@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Um erro ocorreu ao processar sua solicitação:";
}

<div class="header-title"><h3>@ViewBag.Title</h3></div>
@if (!string.IsNullOrWhiteSpace(ViewBag.Message))
{
    <h5 class="text-danger">@ViewBag.Message</h5>
}
else
{
    <h5 class="text-danger">@Model.Exception.Message</h5>
}
<a class="btn btn-default pull-left" href="javascript:history.back()">OK</a>